@{
}

@model IList<TagItem>

<div class="mb-4">
    <h3 class="text-success mb-2">
        Các thẻ
    </h3>

    @foreach (var tag in Model)
    {

        <a asp-controller="Blog"
       asp-action="Tag"
       asp-route-slug=@tag.UrlSlug
           title="@tag.Name">
            @tag.Name
        </a>
    }
</div>
